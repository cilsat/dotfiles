! xft settings
Xft.antialias: 1
Xft.hinting: 1
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! genereal urxvt settings
URxvt.depth: 32
URxvt.loginShell: true
URxvt.saveLines: 9999terminal.svg

! font settings
URxvt.letterSpace: -1
!URxvt.font: xft:ypn envypn:size=9, xft:noto mono:size=8.5
URxvt.font: xft:inconsolatalgc nerd font:size=8, xft:ypn envypn:size=9

! window settings
URxvt.iconFile: /usr/share/icons/Papirus-Dark/32x32/apps/utilities-terminal.svg
URxvt.geometry: 80x57+900+0
URxvt.internalBorder: 0
!URxv.*externalBorder: 0
URxvt.borderLess: false
URxvt.borderColor: 00
URxvt.scrollBar: false
!URxvt*transparent: false
!URxvt*shading: 30
!URxvt*depth: 32
!URxvt*background: rgba:0000/0000/0200/c800

! perl extensions
URxvt.perl-ext-common: default,url-select,keyboard-select
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
