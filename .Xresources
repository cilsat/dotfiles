!cursor
xcursor*theme:  Bluecurve

!font rendering settings
Xft.antialias:          1
Xft.hinting:            1
Xft.autohint:           0
Xft.hintstyle:          hintslight
Xft.dpi:                96
Xft.rgba:               rgb 
Xft.lcdfilter:          lcddefault

!xterm settings
xterm.termName:         xterm-256color
xterm.dynamicColors:    true
xterm.utf8:             2
xterm.faceName:         Ubuntu Mono derivative Powerline:size=9

!URxvt settings
!URxvt*termName:         tmux-256color
URxvt.depth:            32
URxvt.loginShell:       true
URxvt.iconFile:         /usr/share/icons/Paper/48x48/apps/utilities-terminal.png
URxvt.saveLines:        9999

URxvt.geometry:         80x57+900+0
URxvt.internalBorder:   0
!URxv.*externalBorder:   0
URxvt.borderLess:       false
URxvt.borderColor:      00
URxvt.scrollBar:        false

URxvt.skipBuiltinGlyphs: true
URxvt.cursorBar:        false
URxvt.cursorBlink:      true

!URxvt*transparent:      true
!URxvt*shading:          30
!URxvt*depth:            32
!URxvt*background:       rgba:0000/0000/0200/c800

URxvt.letterSpace:      -1

!TrueType fonts
URxvt.font:             xft:DejaVu Sans Mono:pixelsize=10.5:book,xft:Hack:pixelsize=10.5
!URxvt.font:             xft:Droid Sans Mono Slashed for Powerline:size=9,xft:Hack:size=9
!URxvt.font:             xft:Source Code Pro:pixelsize=10.5,xft:DejaVu Sans Mono:pixelsize=10.5

!Bitmap fonts
!URxvt.font:             xft:envypn-9

