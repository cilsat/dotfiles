!cursor
xcursor*theme:  Bluecurve

!font rendering settings
Xft*antialias:          1
Xft*hinting:            1
Xft*autohint:           0
Xft*hintstyle:          hintfull
Xft*dpi:                96
Xft*rgba:               rgb 

!xterm settings
xterm*termName:         xterm-253color
xterm*dynamicColors:    true
xterm*utf8:             2
xterm*faceName:         Ubuntu Mono derivative Powerline:size=9

!URxvt settings
URxvt*termName:         rxvt-unicode-256color
URxvt.depth:            32
URxvt*loginShell:       true
URxvt*iconFile:         /usr/share/icons/Numix-Circle/48x48/apps/final-term.svg
URxvt.saveLines:        9999

URxvt.geometry:         90x52+815+26
URxvt*internalBorder:   0
!URxvt*externalBorder:   0
URxvt*borderLess:       false
URxvt*borderColor:      00
URxvt*scrollBar:        false

!Rxvt*transparent:      true
!Rxvt*shading:          30
!Rxvt*depth:            32
!Rxvt*background:       rgba:0000/0000/0200/c800

URxvt.letterSpace:      -2

!TrueType fonts
URxvt.font:             xft:DejaVu Sans Mono for Powerline:pixelsize=11.5,xft:Terminess Powerline-11
URxvt.boldFont:         xft:DejaVu Sans Mono for Powerline:pixelsize=11.5:bold,xft:Terminess Powerline-11
URxvt.italicFont:       xft:DejaVu Sans Mono for Powerline:pixelsize=11.5:italic,xft:Terminess Powerline-11
!Rxvt.font:             xft:Ubuntu Mono-10.5,xft:DejaVu Sans Mono for Powerline-11,xft:Terminess Powerline-12
!Rxvt.boldFont:         xft:Ubuntu Mono-10.5:bold,xft:DejaVu Sans Mono for Powerline-11:bold,xft:Terminess Powerline-12
!Rxvt.italicFont:       xft:Ubuntu Mono-10.5:italic,xft:DejaVu Sans Mono for Powerline-11:italic,xft:Terminess Powerline-12

!Bitmap fonts
!Rxvt.font:             xft:Tamzen:size=13,xft:Terminess Powerline:size=9
!Rxvt.boldFont:         xft:Tamzen:bold:size=13,xft:Terminess Powerline:bold:size=9
!Rxvt.italicFont:       xft:Tamzen:size=13,xft:Terminess Powerline:size=9
!Rxvt*font:             xft:Terminess Powerline:size=9
!Rxvt*boldFont:         xft:Termines Powerline:bold:size=9
!Rxvt.font:             xft:envypn-9
!Rxvt.boldFont:         xft:Tamzen-9:bold
!Rxvt.italicFont:       xft:envypn-9:italic
!Rxvt.font:             xft:Dina Powerline:size=9,xft:Terminess Powerline:size=9
!Rxvt.boldFont:         xft:Dina Powerline:bold:size=9,xft:Terminess Powerline:size=9:bold
!Rxvt.italicFont:       xft:Dina Powerline:italic:size=9,xft:Terminess Powerline:size=9:italic

