!cursor
xcursor*theme:  Bluecurve

!font rendering settings
Xft*antialias:          1
Xft*hinting:            1
Xft*autohint:           0
Xft*hintstyle:          hintfull
Xft*dpi:                96
Xft*rgba:               rgb

!xterm settings
xterm*termName:         xterm-256color
xterm*dynamicColors:    true
xterm*utf8:             2
xterm*faceName:         Ubuntu Mono derivative Powerline:size=9

!URxvt settings
URxvt*termName:         rxvt-unicode-256color
URxvt.depth:            32
URxvt*loginShell:       true
URxvt*iconFile:         /usr/share/icons/Numix-Circle/48x48/apps/final-term.svg
URxvt.saveLines:        9999

URxvt.geometry:         90x58+10+35
URxvt*internalBorder:   0
URxvt*externalBorder:   0
URxvt*borderLess:       false
URxvt.borderColor:      00
URxvt*scrollBar:        false

URxvt.letterSpace:      -1

!URxvt*font:             xft:Source Code Pro for Powerline:pixelsize=13,xft:Ubuntu Mono derivative Powerline:pixelsize=13
!URxvt*boldFont:         xft:Source Code Pro for Powerline:bold:pixelsize=12,xft:Ubuntu Mono derivative Powerline:bold:pixelsize=13
!URxvt*font:             xft:Meslo LG L DZ for Powerline:pixelsize=12
!URxvt*fontBold:         xft:Meslo LG L DZ for Powerline:bold:pizelsize=12
!URxvt*font:             xft:Droid Sans Mono Dotted for Powerline:pixelsize=13,xft:Ubuntu Mono derivative Powerline-9
!URxvt*boldFont:         xft:Droid Sans Mono Dotted for Powerline:bold:pixelsize=13,xft:Ubuntu Mono derivative Powerline-9:bold
!URxvt*font:            xft:Envy Code R for Powerline-8
!URxvt*boldFont:        xft:Envy Code R for Powerline-8:bold
!URxvt*font: 		xft:Bitstream Vera Sans Mono-8,xft:Terminess Powerline:pixelsize=8
!URxvt*boldFont:         xft:Bitstream Vera Sans Mono-8,xft:Terminess Powerline:pixelsize=8
URxvt*font:             xft:Ubuntu Mono derivative Powerline:pixelsize=14
URxvt*boldFont:         xft:Ubuntu Mono derivative Powerline:bold:pixelsize=14
!URxvt*font:             xft:Consolas for Powerline:size=9,xft:Droid Sans Mono Slashed for Powerline:size=10
!URxvt*boldFont:         xft:Consolas for Powerline:bold:size=9,xft:Droid Sans Mono Slased for  Powerline:bold:size=10
!URxvt*font:             xft:Terminess Powerline:size=9
!URxvt*boldFont:         xft:Terminess Powerline:bold:size=9

