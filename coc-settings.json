{
  "suggest.enablePreview": true,
  "suggest.detailMaxLength": 50,
  "suggest.detailField": "preview",
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 500,
  "suggest.minTriggerInputLength": 0,
  "suggest.triggerCompletionWait": 150,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.maxCompleteItemCount": 30,
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "suggest.snippetIndicator": "►",
  "coc.preferences.formatOnSaveFiletypes": ["python", "cpp"],
  "diagnostic.infoSign": "▍",
  "diagnostic.warningSign": "▍",
  "diagnostic.errorSign": "▍",
  "diagnostic.hintSign": "▍",
  "list.maxPreviewHeight": 25,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "args": ["--log-file=/tmp/ccls.log"],
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": ["compile_commands.json", ".ccls", ".vim/", ".git/"],
      "initializationOptions": {
        "cache": {
          "directory": "/home/cilsat/.cache/ccls",
          "format": "binary"
        },
        "index": {
          "initialWhitelist": [".h$", ".hpp$", ".c$", ".cc$", ".cpp$"],
          "initialBlacklist": ["."]
        }
      }
    },
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    },
    "dockerfile": {
      "command": "docker-langserver",
      "args": ["--stdio"],
      "filetypes": ["dockerfile"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/"],
      "filetypes": ["go"]
    }
  },
  "python.disableInstallationCheck": true,
  "python.jediEnabled": true,
  "python.jediPath": "/usr/lib/python3.8/site-packages",
  "python.venvFolders": [
    ".local/share/venv",
    ".cache/pypoetry/virtualenvs",
    ".local/share/conda/envs"
  ],
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "/usr/bin/pylint",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "python.formatting.blackArgs": ["--fast"]
}
